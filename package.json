{
  "name": "figma-pexels",
  "version": "1.0.8",
  "description": "A Figma plugin to insert photos from the popular free stock photography site Pexels.",
  "license": "ISC",
  "scripts": {
    "build": "/usr/local/bin/node node_modules/.bin/webpack --mode=production",
    "build:watch": "/usr/local/bin/node node_modules/.bin/webpack --mode=development --watch",
    "prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}'",
    "api:watch": "npx json-server --watch api/db.json"
  },
  "dependencies": {
    "@babel/preset-env": "^7.9.5",
    "@types/node": "^13.13.0",
    "axios": "^0.19.2",
    "figma-plugin-ds": "git+https://github.com/craigmdennis/figma-plugin-ds.git#add-package-json",
    "json-server": "^0.16.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.3.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.4",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
